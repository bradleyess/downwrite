{
  "name": "redownwrite",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "react-scripts": "^1.0.10"
  },
  "dependencies": {
    "draft-js": "^0.10.1",
    "glamor": "^3.0.0-2",
    "glamor-jsxstyle": "^3.0.0-2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "serve": "^6.0.2",
    "slate": "^0.20.4",
    "slate-markdown": "^0.1.1",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "serve -s build",
    "dev": "concurrently -c 'bgBlue.bold,bgYellow.bold' --names 'MockDB,Server' 'npm run dev:db' 'npm run dev:server'",
    "dev:server": "react-scripts start",
    "dev:db": "json-server --watch db.json -p 7171",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "go": "now && now alias"
  },
  "now": {
    "alias": "redownwrite"
  },
  "proxy": "http://localhost:7171"
}
