{
	"name": "redownwrite",
	"version": "0.1.0",
	"private": true,
	"devDependencies": {
		"enzyme": "^3.1.0",
		"flow-bin": "^0.57.3",
		"husky": "^0.14.3",
		"lint-staged": "^4.3.0",
		"react-test-renderer": "^16.0.0"
	},
	"dependencies": {
		"date-fns": "^1.29.0",
		"draft-js": "^0.10.4",
		"file-saver": "^1.3.3",
		"glamor": "^2.20.40",
		"lodash.isempty": "^4.4.0",
		"react": "^16.0.0",
		"react-cookie": "^2.1.1",
		"react-dom": "^16.0.0",
		"react-icons": "^2.2.7",
		"react-media": "^1.6.1",
		"react-router-dom": "^4.2.2",
		"react-scripts": "^1.0.14",
		"serve": "^6.3.1",
		"uuid": "^3.1.0"
	},
	"scripts": {
		"start": "serve -s build",
		"build": "react-scripts build",
		"dev": "react-scripts start",
		"test": "flow && react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"go": "now && now alias",
		"format": "prettier --write 'src/**/*.js'",
		"precommit": "lint-staged"
	},
	"prettier": {
		"printWidth": 95,
		"parser": "flow",
		"semi": false,
		"singleQuote": true,
		"useTabs": true,
		"bracketSpacing": true,
		"jsxBracketSameLine": true
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"git add"
		]
	},
	"now": {
		"alias": "redownwrite"
	}
}
