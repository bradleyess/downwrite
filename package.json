{
  "name": "redownwrite",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "flow-bin": "^0.52.0",
    "json-server": "^0.12.0",
    "prettier-miscellaneous": "^1.5.3",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "glamor": "^2.20.39",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "recompose": "^0.24.0",
    "serve": "^6.0.6",
    "slate": "^0.21.1",
    "slate-markdown": "^0.1.1",
    "typeface-roboto-mono": "0.0.31",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "serve -s build",
    "dev": "concurrently -c 'bgBlue.bold,bgYellow.bold' --names 'MockDB,Server' 'npm run dev:db' 'npm run dev:server'",
    "dev:server": "react-scripts start",
    "dev:db": "json-server --watch db.json -p 7171",
    "build": "react-scripts build",
    "test": "concurrently -c 'bgMagenta.bold,bgGreen.bold' --names 'Flow,Jest' 'flow' 'npm run test:unit'",
    "test:unit": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "go": "now && now alias",
    "flow": "flow",
    "lint": "prettier --space-before-function-paren true --single-quote --braces-spacing true --jsx-single-quote --jsx-bracket-same-line --no-semi --write --parser 'flow' 'src/**/*.js'"
  },
  "now": {
    "alias": "redownwrite"
  },
  "proxy": "http://localhost:7171"
}
