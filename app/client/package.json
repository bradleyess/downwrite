{
	"name": "redownwrite",
	"version": "0.1.0",
	"private": true,
	"devDependencies": {
		"enzyme": "^3.1.0",
		"enzyme-adapter-react-16": "^1.0.2",
		"enzyme-to-json": "^3.1.4",
		"enzyme-wait": "^1.0.9",
		"fbjs": "^0.8.16",
		"flow-bin": "^0.59.0",
		"husky": "^0.14.3",
		"jest-fetch-mock": "^1.2.1",
		"jest-glamor-react": "^3.1.2",
		"lint-staged": "^6.0.0",
		"prettier": "^1.8.2",
		"puppeteer": "^0.13.0",
		"react-addons-test-utils": "^15.6.2",
		"react-test-renderer": "^16.0.0",
		"w3c-xmlhttprequest": "^2.1.2"
	},
	"lint-staged": {
		"*.js": [
			"prettier --parser flow --write",
			"git add"
		],
		"*.css": [
			"prettier --parser postcss --write",
			"git add"
		]
	},
	"dependencies": {
		"date-fns": "^1.29.0",
		"draft-js": "^0.10.4",
		"draft-js-markdown-plugin": "^1.1.0",
		"draft-js-plugins-editor": "^2.0.3",
		"draft-js-prism-plugin": "^0.1.1",
		"fg-loadcss": "^2.0.1",
		"file-saver": "^1.3.3",
		"front-matter": "^2.3.0",
		"glamor": "^2.20.40",
		"immutable": "^3.8.2",
		"jwt-decode": "^2.2.0",
		"lodash": "^4.17.4",
		"markdown-draft-js": "^0.4.5",
		"prismjs": "^1.8.4",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-dropzone": "^4.2.3",
		"react-loadable": "^5.3.1",
		"react-markdown": "^3.0.2",
		"react-media": "^1.6.1",
		"react-prism": "^4.3.1",
		"react-remarkable": "^1.1.3",
		"react-router-dom": "^4.2.2",
		"react-scripts": "^1.0.17",
		"serve": "^6.4.0",
		"universal-cookie": "^2.1.0",
		"uuid": "^3.1.0"
	},
	"scripts": {
		"start": "serve -s build",
		"build": "react-scripts build",
		"dev": "react-scripts start",
		"test": "react-scripts test --env=jsdom",
		"analysis": "flow",
		"lint": "yarn eslint ./src/**/*.js",
		"format": "prettier --write 'src/**/*.js' --parser flow",
		"eject": "react-scripts eject",
		"deploy": "now && now alias",
		"precommit": "lint-staged"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"eslintIgnore": [
		"/src/__tests__/*.test.js"
	],
	"now": {
		"alias": [
			"alpx.downwrite.us"
		]
	},
	"jest": {
		"snapshotSerializers": [
			"jest-glamor-react",
			"enzyme-to-json/serializer"
		]
	},
	"repository": "charlespeters/downwrite",
	"author": "Charles âš¡ <charlespeters42@gmail.com>"
}
